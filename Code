// Function to display game title.
void homescreen_sign()
{
    cout << " ----------------------------------------------\n";
    cout << "|            ╔═══╦╗ ╔╦═══╦╗╔═╦═══╗             |\n";
    cout << "|            ║╔═╗║║ ║║╔═╗║║║╔╣╔═╗║             |\n";
    cout << "|            ║║ ╚╣║ ║║╚═╝║╚╝╝║║ ║║             |\n";
    cout << "|            ║║╔═╣║ ║║╔╗╔╣╔╗║║╚═╝║             |\n";
    cout << "|            ║╚╩═║╚═╝║║║╚╣║║╚╣╔═╗║             |\n";
    cout << "|            ╚═══╩═══╩╝╚═╩╝╚═╩╝ ╚╝             |\n";
    cout << " ----------------------------------------------\n";
    cout << "               Swedish for 'Cucumber'\n\n";
}
